    "@vitest/ui": "^4.0.18",
    "babel-plugin-react-compiler": "^1.0.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "jsdom": "^27.4.0",
    "knip": "5.82.1",
    "madge": "^8.0.0",
    "msw": "^2.12.7",
    "postcss": "^8.5.6",
