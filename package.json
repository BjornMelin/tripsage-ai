    "js-tiktoken": "^1.0.21",
    "katex": "^0.16.27",
    "lucide-react": "^0.562.0",
    "mammoth": "^1.11.0",
    "motion": "12.27.5",
    "next": "^16.1.4",
    "next-themes": "^0.4.6",
    "pdf-parse": "^2.4.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "6.1.0",
    "react-hook-form": "^7.71.1",
    "recharts": "^3.6.0",
    "require-in-the-middle": "^8.0.1",
    "resend": "^6.7.0",
    "shiki": "^3.21.0",
    "sonner": "^2.0.7",
    "streamdown": "^2.1.0",
    "stripe": "20.2.0",
    "tailwind-merge": "^3.4.0",
