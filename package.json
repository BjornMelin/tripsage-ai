  "engines": {
    "node": "24.x",
    "pnpm": ">=9.0.0"
  },
  "pnpm": {
    "packageExtensions": {
      "@hookform/resolvers@*": {
        "dependencies": {
          "zod": "^4.2.1"
        }
      },
      "openai@*": {
        "dependencies": {
          "zod": "3.25.76"
        }
      }
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "openai>zod": "^4.2.0"
      }
    },
    "overrides": {
      "axios": "1.13.2",
      "prismjs": ">=1.30.0",
      "undici": "5.29.0",
      "brace-expansion": ">=2.0.2"
    },
    "ignoredBuiltDependencies": [
      "esbuild",
      "msw"
    ]
  }
}
