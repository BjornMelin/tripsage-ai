{
  "name": "tripsage-ai-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "biome lint ./src ./e2e ./scripts",
    "format:biome": "biome format ./src --write",
    "biome:check": "biome check ./src ./e2e ./scripts",
    "biome:fix": "biome check ./src ./e2e ./scripts --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:short": "vitest run --test-timeout=3000 --hook-timeout=3000 --teardown-timeout=3000",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage.enabled --coverage.provider=v8",
    "test:benchmark": "npx tsx scripts/benchmark-tests.ts",
    "test:quick": "vitest run --reporter=dot",
    "test:ci": "vitest run --pool=threads --maxWorkers=2 --reporter=dot",
    "test:dev-fast": "vitest --pool=threads --maxWorkers=50% --reporter=default",
    "test:unit": "vitest run --project=unit",
    "test:components": "vitest run --project=component",
    "test:api": "vitest run --project=api",
    "test:integration": "vitest run --project=integration",
    "test:upstash:unit": "vitest run src/test/upstash/upstash.unit.test.ts",
    "test:upstash:int": "UPSTASH_USE_EMULATOR=1 vitest run --project=integration src/__tests__/contracts/upstash-int.integration.test.ts",
    "test:upstash:smoke": "UPSTASH_SMOKE=1 vitest run --project=integration src/__tests__/contracts/upstash.smoke.integration.test.ts",
    "test:changed": "vitest run --changed",
    "test:coverage:shard": "vitest run --coverage.enabled --coverage.provider=v8 --reporter=blob",
    "test:merge": "vitest run --merge-reports",
    "test:e2e": "playwright test",
    "ops": "tsx scripts/ops.ts",
    "boundary:check": "node ../scripts/check-boundaries.js",
    "codemod:ai-route:dry": "npx jscodeshift -t ../scripts/codemods/ai-route-v6-upgrade.js \"src/app/**/route.ts\" --parser=ts -d -p",
    "codemod:ai-route": "npx jscodeshift -t ../scripts/codemods/ai-route-v6-upgrade.js \"src/app/**/route.ts\" --parser=ts",
    "codemod:ai-messages:dry": "npx jscodeshift -t ../scripts/codemods/ai-chat-messages-convert.js \"src/app/**/route.ts\" --parser=ts -d -p",
    "codemod:ai-messages": "npx jscodeshift -t ../scripts/codemods/ai-chat-messages-convert.js \"src/app/**/route.ts\" --parser=ts",
    "codemod:tests-env:dry": "npx jscodeshift -t ../scripts/codemods/tests-env-stub.js \"src/**/*.{test.ts,test.tsx}\" --parser=ts -d -p",
    "codemod:tests-env": "npx jscodeshift -t ../scripts/codemods/tests-env-stub.js \"src/**/*.{test.ts,test.tsx}\" --parser=ts",
    "codemod:validate": "pnpm biome:fix && pnpm type-check && pnpm test:run",
    "codemod:check-idempotency": "pnpm codemod:ai-route:dry && pnpm codemod:ai-messages:dry && pnpm codemod:tests-env:dry && git diff --quiet"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "3.0.0-beta.60",
